{
  "name": "typed-html-templates",
  "version": "1.0.0",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "license": "MIT",
  "keywords": [
    "html",
    "htm",
    "tagged-template-literals",
    "typescript",
    "type-safety",
    "jsx-alternative",
    "hyperscript",
    "templating"
  ],
  "devDependencies": {
    "@types/node": "^22.10.2",
    "pridepack": "2.6.4",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "htm": "^3.1.1"
  },
  "scripts": {
    "prepublishOnly": "pridepack clean && pridepack build",
    "build": "pridepack build",
    "type-check": "tsc --project tsconfig.typecheck.json",
    "clean": "pridepack clean",
    "watch": "pridepack watch",
    "start": "pridepack start",
    "dev": "pridepack dev",
    "test": "vitest",
    "release:patch": "npm version patch && git push origin HEAD --follow-tags",
    "release:minor": "npm version minor && git push origin HEAD --follow-tags",
    "release:major": "npm version major && git push origin HEAD --follow-tags"
  },
  "private": false,
  "description": "HTM-compatible html tagged templates with strong TypeScript hole typing and DOM-aware inference.",
  "repository": {
    "url": "https://github.com/doeixd/typed-html-templates",
    "type": "git"
  },
  "homepage": "https://github.com/doeixd/typed-html-templates",
  "bugs": {
    "url": "https://github.com/doeixd/typed-html-templates/issues"
  },
  "author": "Patrick Glenn",
  "publishConfig": {
    "access": "public"
  },
  "types": "./dist/types/index.d.ts",
  "main": "./dist/cjs/production/index.js",
  "module": "./dist/esm/production/index.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "development": {
        "require": "./dist/cjs/development/index.js",
        "import": "./dist/esm/development/index.js"
      },
      "require": "./dist/cjs/production/index.js",
      "import": "./dist/esm/production/index.js"
    }
  },
  "typesVersions": {
    "*": {}
  }
}
